#!/usr/bin/python

import sys

def gcd(a, b):
    while b != 0:
        a, b = b, a % b
    return a

def pollards_rho(n):
    if n % 2 == 0:
        return 2
    x = 2; y = 2; d = 1
    f = lambda x: (x**2 + 1) % n
    while d == 1:
        x = f(x)
        y = f(f(y))
        d = gcd(abs(x-y), n)
    return d

def main():
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        return

    filename = sys.argv[1]
    with open(filename, 'r') as file:
        for line in file:
            n = int(line.strip())
            p = pollards_rho(n)
            q = n // p
            print(f"{n}={p}*{q}")

if __name__ == "__main__":
    main()
